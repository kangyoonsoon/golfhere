<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="coursens">

  	<!-- 골프장 1개 정보가져오기 -->
  	<select id="get_course" parameterType="int" resultType="course">  		 
    	select * from course where course_num = #{course_num}
    </select>

    <!-- 게시판 글 저장 -->
    <select id="course_list" resultType="course">
	     		 
    	select * from course
    
    </select>
  
</mapper>